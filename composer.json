{
    "name": "jessyledama/kompressor",
    "description": "Lossless-style image compressor for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JessyLedama\\Kompressor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JessyLedama\\Kompressor\\KompressorServiceProvider"
            ],
            "aliases": {
                "Kompressor": "JessyLedama\\Kompressor\\Facades\\Kompressor"
            }
        }
    },
    "require": {
        "php": ">=8.1",
        "intervention/image": "^3.0",
        "spatie/image-optimizer": "^1.6"
    }
}
